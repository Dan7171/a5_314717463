{
  "PlpMain": {
    "Project": "example",
    "Name": "navigate",
    "Type": "Glue"
  },
  "GlueFramework": "ROS",
  "ModuleResponse": {
    "ResponseRules": [
      {
        "Response": "eSuccess",
        "ConditionCodeWithLocalVariables": " navigateSkillSuccess"
      },
      {
        "Response": "eFailed",
        "ConditionCodeWithLocalVariables": " True"
      }
    ]
  },
  "ModuleActivation": {
    "RosService": {
      "ImportCode": [
        {
          "From": "geometry_msgs.msg",
          "Import": [
            "Point"
          ]
        },
        {
          "From": "task3_env.srv",
          "Import": [
            "navigateResponse",
            "navigate"
          ]
        }
      ],
      "ServiceName": "navigate",
      "ServicePath": "/navigate",
      "ServiceParameters": [
        {
          "ServiceFieldName": "location",
          "AssignServiceFieldCode": "# location_to_ros_service is the value to set to location parameter"
        }
      ]
    }
  },
  "LocalVariablesInitialization": [
    {
      "InputLocalVariable": "location_to_ros_service",
      "FromGlobalVariable": "navToLoc"
    },
    {
      "FromROSServiceResponse": true,
      "LocalVariableName": "navigateSkillSuccess",
      "VariableType": "bool",
      "AssignmentCode": [
        "ros_service_response = __input.success",
        "navigateSkillSuccess=ros_service_response"
      ],
      "ImportCode": [
        {
          "From": "std_msgs.msg",
          "Import": [
            "Bool"
          ]
        }
      ]
    }
  ]
}