{
  "PlpMain": {
    "Project": "collect_toys",
    "Name": "navigate",
    "Type": "Glue"
  },
  "GlueFramework": "ROS",
  "ModuleResponse": {
    "ResponseRules": [
      {
        "Response": "eSuccess",
        "ConditionCodeWithLocalVariables": " navigateSkillSuccess"
      },
      {
        "Response": "eFailed",
        "ConditionCodeWithLocalVariables": " True"
      }
    ]
  },
  "ModuleActivation": {
    "RosService": {
      "ImportCode": [
        {
          "From": "geometry_msgs.msg",
          "Import": [
            "Point"
          ]
        },
        {
          "From": "task3_env.srv",
          "Import": [
            "navigate",
            "navigateResponse"
          ]
        }
      ],
      "ServiceName": "navigate",
      "ServicePath": "/navigate",
      "ServiceParameters": [
        {
          "ServiceFieldName": "location",
          "AssignServiceFieldCode": "navToLocLocalVar"
        }
      ]
    }
  },
  "LocalVariablesInitialization": [
    {
      "InputLocalVariable": "navToLocLocalVar",
      "FromGlobalVariable": "navToLoc"
    },
    {
      "FromROSServiceResponse": true,
      "LocalVariableName": "navigateSkillSuccess",
      "VariableType": "bool",
      "AssignmentCode": [
        "navigateSkillSuccess=__input.success"
      ],
      "ImportCode": [
        {
          "From": "std_msgs.msg",
          "Import": [
            "Bool"
          ]
        }
      ]
    }
  ]
}