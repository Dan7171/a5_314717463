{
  "PlpMain": {
    "Project": "example",
    "Name": "place",
    "Type": "Glue"
  },
  "GlueFramework": "ROS",
  "ModuleResponse": {
    "ResponseRules": [
      {
        "Response": "eSuccess",
        "ConditionCodeWithLocalVariables": " placeSkillSuccess"
      },
      {
        "Response": "eFailed",
        "ConditionCodeWithLocalVariables": " True"
      }
    ]
  },
  "ModuleActivation": {
    "RosService": {
      "ImportCode": [
        {
          "From": "geometry_msgs.msg",
          "Import": [
            "Point"
          ]
        },
        {
          "From": "task3_env.srv",
          "Import": [
            "placeResponse",
            "place"
          ]
        }
      ],
      "ServiceName": "place",
      "ServicePath": "/place",
      "ServiceParameters": []
    }
  },
  "LocalVariablesInitialization": [
    {
      "FromROSServiceResponse": true,
      "LocalVariableName": "placeSkillSuccess",
      "VariableType": "bool",
      "AssignmentCode": [
        "ros_service_response = __input.success",
        "placeSkillSuccess = ros_service_response"
      ],
      "ImportCode": [
        {
          "From": "std_msgs.msg",
          "Import": [
            "Bool"
          ]
        }
      ]
    }
  ]
}