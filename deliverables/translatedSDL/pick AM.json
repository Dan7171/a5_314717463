{
  "PlpMain": {
    "Project": "example",
    "Name": "pick",
    "Type": "Glue"
  },
  "GlueFramework": "ROS",
  "ModuleResponse": {
    "ResponseRules": [
      {
        "Response": "eSuccess",
        "ConditionCodeWithLocalVariables": " pickSkillSuccess"
      },
      {
        "Response": "eFailed",
        "ConditionCodeWithLocalVariables": " True"
      }
    ]
  },
  "ModuleActivation": {
    "RosService": {
      "ImportCode": [
        {
          "From": "geometry_msgs.msg",
          "Import": [
            "Point"
          ]
        },
        {
          "From": "task3_env.srv",
          "Import": [
            "pick",
            "pickResponse"
          ]
        }
      ],
      "ServiceName": "pick",
      "ServicePath": "/pick",
      "ServiceParameters": [
        {
          "ServiceFieldName": "toy_type",
          "AssignServiceFieldCode": "# this value will be set to toy_type and will be sent to ros service"
        }
      ]
    }
  },
  "LocalVariablesInitialization": [
    {
      "InputLocalVariable": "toy_type_to_ros_service",
      "FromGlobalVariable": "chosenColorParam"
    },
    {
      "FromROSServiceResponse": true,
      "LocalVariableName": "pickSkillSuccess",
      "VariableType": "bool",
      "AssignmentCode": [
        "ros_service_response = __input.success",
        "pickSkillSuccess=ros_service_response"
      ],
      "ImportCode": [
        {
          "From": "std_msgs.msg",
          "Import": [
            "Bool"
          ]
        }
      ]
    }
  ]
}